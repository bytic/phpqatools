{
  "name": "bytic/phpqatools",
  "description": "Meta package for installing all PHP QA tools you'll ever need",
  "type": "metapackage",
  "keywords": [
    "php",
    "qa", "quality", "code quality",
    "stylecheck", "unittest"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Solomon Gabriel",
      "email": "hello@gabrielsolomon.ro",
      "homepage": "http://gabrielsolomon.ro",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6,<8.0-dev",
    "bytic/codeception": "^1.0",
    "friendsofphp/php-cs-fixer": "^2.8",
    "pdepend/pdepend": "^2.5",
    "phploc/phploc": "^4.0",
    "phpmd/phpmd": "^2.6.0",
    "phpmetrics/phpmetrics": "^2.3.2",
    "phpro/grumphp": ">=0.11.0,<1.0.0,!=0.11.4",
    "phpunit/phpunit": "^5.7|^6.5",
    "sebastian/phpcpd": "^3.0",
    "seld/jsonlint": "^1.6",
    "sensiolabs/security-checker": "^4.1",
    "sllh/composer-versions-check": "^2.0",
    "squizlabs/php_codesniffer": "^3.2",
    "wimg/php-compatibility": "^8.0"
  },
  "suggest": {
  },
  "config": {
    "sort-packages": true
  }
}